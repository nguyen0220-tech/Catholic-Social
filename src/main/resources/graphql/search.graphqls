extend type Query {
    searchHistory(page:Int!, size:Int!):ListResponse_SearchDTO
}

extend type Mutation {
    createSearch(request:SearchRequest!):GraphqlResponse_String!
    deleteSearch(searchId:ID):GraphqlResponse_String!
}

input SearchRequest{
    keyword:String
}


type ListResponse_SearchDTO{
    data:[SearchDTO]
    pageInfo:PageInfo
}

type SearchDTO{
    id:ID!
    keyword:String!
    createdAt:String!
}

type PageInfo{
    page:Int!
    size:Int!
    hasNext:Boolean!
}
